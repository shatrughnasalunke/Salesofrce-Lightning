<aura:component Controller="AccountSubsidiary" implements="force:appHostable,flexipage:availableForAllPageTypes,flexipage:availableForRecordHome,force:hasRecordId,forceCommunity:availableForAllPageTypes,force:lightningQuickAction" access="global" >
    <aura:attribute name="subsidiary" type="List" />
    <aura:attribute name="recordId" type="String" />
    <aura:attribute name="mycolumns" type="List"/>
    <aura:handler name="init" value="{!this}" action="{!c.doInit}"/>
                    <lightning:datatable data="{!v.subsidiary }"
                                     columns="{!v.mycolumns}"
                                     keyField="id"
                                     hideCheckboxColumn="true"/>

</aura:component>